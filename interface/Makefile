all-am: Makefile $(DATA)
installdirs:
	for dir in "$(DESTDIR)$(Applicationsdir)" "$(DESTDIR)$(appsdir)" "$(DESTDIR)$(distpixmapdir)"; do \
	  test -z "$$dir" || $(mkdir_p) "$$dir"; \
	done
